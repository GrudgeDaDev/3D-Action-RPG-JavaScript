{
  "name": "3d-action-rpg-javascript",
  "version": "1.0.0",
  "description": "3D Action RPG built with Babylon.js - Pure JavaScript with no build process",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "dev": "node --watch server.js",
    "dev:static": "npx live-server --port=8080 --watch=src,config,admin,shaders",
    "lint": "npx eslint src/**/*.js",
    "format": "npx prettier --write \"src/**/*.js\" \"admin/**/*.js\" \"config/**/*.json\"",
    "validate-config": "node scripts/validateConfig.js",
    "test": "echo \"No tests yet\" && exit 0",
    "deploy": "puter deploy",
    "deploy:check": "puter whoami && echo 'Ready to deploy!'",
    "puter:login": "puter login",
    "puter:secrets": "echo 'Set GEMINI_API_KEY: puter secret set GEMINI_API_KEY your_key_here'"
  },
  "keywords": [
    "babylonjs",
    "3d",
    "rpg",
    "game",
    "javascript",
    "webgl"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "eslint": "^8.55.0",
    "live-server": "^1.2.2",
    "prettier": "^3.1.1"
  },
  "dependencies": {
        "@google/generative-ai": "^0.24.1",
        "babylon": "^6.18.0",
    "cors": "^2.8.5",
    "express": "^4.22.1",
    "js": "^0.1.0",
    "lib": "^5.1.0",
    "node.js": "^0.0.1-security"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
